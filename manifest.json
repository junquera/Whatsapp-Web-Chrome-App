{
	"manifest_version": 2,
	"name": "WhatsApp Web",
	"description": "WhatsApp Web application",
	"version": "1.1",
	"icons": {
		"128": "128.png"
	},
	"app": {
		"launch": {
			"web_url": "http://web.whatsapp.com/"
		}
	},
	"browser_action": {
    	"default_icon": "128.png",
    	"default_popup": "index.html",
    	"default_title": "WhatsApp Web"
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
	]
}